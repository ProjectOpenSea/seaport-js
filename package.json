{
  "name": "consideration-js",
  "version": "0.0.1",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "build": "yarn hardhat compile && yarn tsc -p tsconfig-build.json",
    "check-types": "tsc --noEmit -p src/__tests__/tsconfig.json",
    "check-types:incremental": "yarn check-types --incremental",
    "eslint:check": "eslint . --max-warnings 0 --ext .js,.jsx,.ts,.tsx",
    "lint": "concurrently \"yarn check-types\" \"yarn eslint:check\" \"yarn prettier:check\" \"yarn prettier:check:package.json\"",
    "prepare": "husky install && yarn build",
    "test": "yarn hardhat test",
    "prettier:check": "prettier --check .",
    "prettier:check:package.json": "yarn prettier-package-json --list-different",
    "prettier:fix": "prettier --write ."
  },
  "dependencies": {
    "@0xsequence/multicall": "^0.36.7",
    "consideration": "git+ssh://git@github.com/ProjectOpenSea/consideration.git#44efe82651a30e80c2eb0f038f456ac58075395e",
    "ethers": "^5.6.0",
    "merkletreejs": "^0.2.31"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@rari-capital/solmate": "^6.2.0",
    "@typechain/ethers-v5": "^9.0.0",
    "@typechain/hardhat": "^5.0.0",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^9.1.0",
    "@types/node": "^12.20.47",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "concurrently": "^7.1.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.2.0",
    "ethereum-waffle": "^3.4.0",
    "hardhat": "^2.9.1",
    "hardhat-gas-reporter": "^1.0.8",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "^2.5.1",
    "prettier-package-json": "^2.6.3",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "sinon": "^13.0.1",
    "sinon-chai": "^3.7.0",
    "solhint": "^3.3.7",
    "solidity-coverage": "^0.7.20",
    "ts-node": "^10.7.0",
    "typechain": "^7.0.1",
    "typescript": "^4.6.2"
  },
  "engines": {
    "node": "16.11.0"
  },
  "lint-staged": {
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ],
    "**/*.{ts,tsx,js,jsx,html,md,mdx,yml,json}": [
      "prettier --write"
    ]
  }
}
